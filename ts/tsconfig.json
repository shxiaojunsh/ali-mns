{
    "compilerOptions": {
        "module": "system",
        "target":"es5",
        "noImplicitAny": false,
        "removeComments": false,
        "preserveConstEnums": true,
        "outFile": "index.js",
        "sourceMap": true,
        "rootDir": "ts/",
        "declaration": true,
        "moduleResolution": "node",
        "lib": ["es2015.promise", "es5", "dom"]
    },
    "exclude": [
        "node_modules",
        "dts"
    ]
}